								params.push([i + 1, 3].join('_') + '=' + c[2]);
								i++;
							}
						}
						params.push('rv=' + Math.random());
						new Image().src = url + '?' + params.join('&');
						i > 0 && collector.length && setTimeout(report, 1000);
					}
